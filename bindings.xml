<Bindings>
	<Binding name="FARSEER_TARGETMORE" category="BINDING_CATEGORY_FARSEER">
		Farseer_ToggleTargetMode()
	</Binding>
	<Binding name="FARSEER_TARGETLESS" category="BINDING_CATEGORY_FARSEER">
		Farseer_ToggleTargetModeReverse()
	</Binding>
	<Binding name="FARSEER_TARGET1" category="BINDING_CATEGORY_FARSEER">
		Farseer_SetTargetMode(1)
	</Binding>
	<Binding name="FARSEER_TARGET2" category="BINDING_CATEGORY_FARSEER">
		Farseer_SetTargetMode(2)
	</Binding>
	<Binding name="FARSEER_TARGET3" category="BINDING_CATEGORY_FARSEER">
		Farseer_SetTargetMode(3)
	</Binding>
	<Binding name="FARSEER_TARGET4" category="BINDING_CATEGORY_FARSEER">
		Farseer_SetTargetMode(4)
	</Binding>
	<Binding name="FARSEER_TARGET5" category="BINDING_CATEGORY_FARSEER">
		Farseer_SetTargetMode(5)
	</Binding>
</Bindings>
